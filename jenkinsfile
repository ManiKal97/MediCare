pipeline{
agent any 
  
stages{
    stage('Clone Repository')
       {
           steps{
                 git 'https://github.com/ManiKal97/MediCare.git'
                }
        }
    stage('Build Docker Image')
       {
           steps{
                 script {
                  bat 'docker build -t medicare:latest .'
                 }
                }
        }
        
}

}




